{
  "type": "module",
  "description": "Form markup language",
  "packageManager": "pnpm@7.12.2",
  "scripts": {
    "test": "vitest",
    "dev:docs": "vitepress dev docs",
    "serve:docs": "vitepress serve docs",
    "build:docs": "vitepress build docs",
    "build:core": "pnpm exec:core build",
    "build:vue": "pnpm exec:vue build",
    "build:editor": "pnpm exec:editor build",
    "build:utils": "pnpm exec:utils build",
    "exec:core": "pnpm -F formkl",
    "exec:vue": "pnpm -F @formkl/vue",
    "exec:editor": "pnpm -F @formkl/editor",
    "exec:utils": "pnpm -F @formkl/utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formkl/formkl.git"
  },
  "keywords": [
    "form",
    "formkl",
    "formjs",
    "form",
    "generator",
    "schema"
  ],
  "author": "thecodeorigin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formkl/formkl/issues"
  },
  "homepage": "https://github.com/formkl/formkl#readme",
  "workspaces": [
    "language",
    "packages/**",
    "sandbox",
    "utils"
  ],
  "dependencies": {
    "@formkl/editor": "workspace:*",
    "@formkl/utils": "workspace:*",
    "@formkl/vue": "workspace:*",
    "formkl": "workspace:*",
    "vitepress": "1.0.0-alpha.20"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "eslint": "^8.23.1",
    "minify": "^9.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "slugify": "^1.6.5",
    "syntax-cli": "^0.1.26",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.4",
    "vue": "^3.2.39",
    "vue-tsc": "^0.40.13"
  }
}
